@font-face {
    font-family: 'arpa_schulbuchroman_web';
    src: url('fonts/arpaschulbuch-roman-webfont.eot');
    src: url('fonts/arpaschulbuch-roman-webfont.eot?#iefix') format('embedded-opentype'),
         url('fonts/arpaschulbuch-roman-webfont.woff2') format('woff2'),
         url('fonts/arpaschulbuch-roman-webfont.woff') format('woff'),
         url('fonts/arpaschulbuch-roman-webfont.ttf') format('truetype'),
         url('fonts/arpaschulbuch-roman-webfont.svg#arpa_schulbuchroman') format('svg');
    font-weight: normal;
    font-style: normal;

}