@font-face {
    font-family: 'SchulbuchNord';
    src: url('fonts/SchulbuchNord-Normal-webfont.eot');
    src: url('fonts/SchulbuchNord-Normal-webfont.eot?#iefix') format('embedded-opentype'),
         url('fonts/SchulbuchNord-Normal-webfont.woff2') format('woff2'),
         url('fonts/SchulbuchNord-Normal-webfont.woff') format('woff'),
         url('fonts/SchulbuchNord-Normal-webfont.ttf') format('truetype'),
         url('fonts/SchulbuchNord-Normal-webfont.svg#schulbuchnordregular') format('svg');
    font-weight: normal;
    font-style: normal;

} 